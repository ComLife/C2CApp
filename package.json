{
  "name": "C2CApp",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start-debugger": "env REACT_DEBUGGER=\"$([ -d '/Applications/React Native Debugger.app' ] && echo \"open 'rndebugger://set-debugger-loc?host=localhost&port=8083' --args\")\" node node_modules/react-native/local-cli/cli.js start --port 8083",
    "start-8083": "react-native start --port=8083",
    "test": "jest",
    "android-debug": "yarn fix-package && react-native run-android",
    "android-debug-8083": "yarn fix-package && yarn jetify && yarn android-clean && react-native run-android --port 8083",
    "android-release": "yarn fix-package && yarn jetify && cd android && ./gradlew clean && ./gradlew assembleRelease && cd -",
    "android-release-install": "yarn fix-package && yarn jetify && cd android && ./gradlew clean && ./gradlew installRelease && cd -",
    "android-clean": "cd android && ./gradlew clean && cd -",
    "android-dev-menu": "adb shell input keyevent 82",
    "android-5.0": "$ANDROID_HOME/tools/emulator @Nexus_4_API_21",
    "android-9.0": "$ANDROID_HOME/tools/emulator @Nexus_7_API_29",
    "android-debug-host-port": "echo 10.0.2.2:8083",
    "android-uninstall": "adb uninstall com.ugame.react.c2capp",
    "ios": "yarn fix-package && react-native run-ios",
    "ios-8083": "yarn fix-package && react-native run-ios --port 8083",
    "ios-5s": "yarn fix-package && react-native run-ios --simulator=\"iPhone 5s\"",
    "ios-5s-8083": "yarn fix-package && react-native run-ios --port 8083 --simulator=\"iPhone 5s\"",
    "ios-xs-max": "yarn fix-package && react-native run-ios --simulator=\"iPhone Xs Max\"",
    "ios-xs-max-8083": "yarn fix-package && react-native run-ios --port 8083 --simulator=\"iPhone Xs Max\"",
    "lint": "node_modules/.bin/eslint --ext .ts --ext .tsx src --fix --cache",
    "pod-setup": "yarn && cd ios && pod install && cd -",
    "postinstall": "yarn fix-package",
    "postuninstall": "yarn fix-package",
    "fix-package": "node scripts/fix-package.ts test 1.0.2",
    "switch-xcscheme-debug": "node scripts/xcscheme-switcher.ts C2CApp-debug",
    "switch-xcscheme-release": "node scripts/xcscheme-switcher.ts C2CApp-release",
    "unittest:run": "node_modules/.bin/jest --coverage -u",
    "test:e2e": "detox test -c ios.sim.debug",
    "test:e2e:build": "detox build"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "@react-native-community/slider": "^2.0.0",
    "@react-native-community/viewpager": "^1.1.7",
    "axios": "^0.21.1",
    "buffer": "^5.4.0",
    "crypto-js": "^3.1.9-1",
    "date-fns": "^1.30.1",
    "decimal.js-light": "^2.5.0",
    "deep-object-diff": "^1.1.0",
    "i18n-js": "^3.3.0",
    "jsrsasign": "^8.0.12",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-animatable": "^1.3.2",
    "react-native-camera": "^3.1.2",
    "react-native-code-push": "^5.6.1",
    "react-native-device-info": "^2.3.2",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-image-picker": "^1.0.2",
    "react-native-kill-app": "^1.0.4",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^1.1.4",
    "react-native-modal": "^11.3.1",
    "react-native-qr-scanner": "^1.3.2",
    "react-native-qrcode-svg": "^5.1.2",
    "react-native-reanimated": "^1.2.0",
    "react-native-scrollable-tab-view": "^0.10.0",
    "react-native-sentry": "^0.43.2",
    "react-native-shadow": "^1.2.2",
    "react-native-slider": "^0.11.0",
    "react-native-smart-gesture-password": "^2.1.0",
    "react-native-snap-carousel": "^3.8.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.6.4",
    "react-native-swiper": "^1.6.0-nightly.3",
    "react-native-touch-id": "^4.4.1",
    "react-native-webview": "^6.9.0",
    "react-navigation": "^3.11.1",
    "react-navigation-hooks": "^1.0.1",
    "react-redux": "^7.1.0",
    "reconnecting-websocket": "^4.1.10",
    "redux": "^4.0.4",
    "redux-logger": "^4.0.0",
    "redux-persist": "^6.0.0-pre2.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "utfx": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/crypto-js": "^3.1.43",
    "@types/i18n-js": "^3.0.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.137",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-native": "^0.60.7",
    "@types/react-native-scrollable-tab-view": "^0.10.0",
    "@types/react-navigation": "^3.0.8",
    "@types/react-redux": "^7.1.2",
    "@types/react-test-renderer": "^16.9.0",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.2.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-native": "^3.6.0",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.3",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.17.1",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.5.3"
  },
  "workspaces": [
    "packages/*"
  ]
}
